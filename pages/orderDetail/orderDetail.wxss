@import "../../utils/commonConfirm.wxss";
.store{
  padding:60rpx;
  background-color: #fff;
  
}
.store .up{
  margin-bottom:20rpx;
}
.storeLable{
  color:#999;
  font-size:28rpx;
  margin-right:40rpx;
}

.storeTittle{
  color:#333;
  font-size:36rpx;

}
.address{
  color:#b1b1b1;
  font-size:28rpx;
}
 .down{
  padding-left:124rpx;
  position: relative;
  width:70%;
  
} 
.down .storeLable{
  
}
.down .address{
 
}
.goPng{
  width:40rpx;
  height:46rpx;
  position: absolute;
  right:-40rpx;
  top:50%;
  transform: translateY(-30%);
}
.detail{
  margin-top:20rpx;
  /* margin-bottom:20rpx; */
  background-color: #fff;
  padding:60rpx;
}
.detailItem{
  margin-bottom:40rpx;
}
.detailLable{
  display:inline-block;
  width:120rpx;
  color:#999;
  font-size:28rpx;
  margin-right:60rpx;
}
.detailValue{
  color:#333;
  font-size:32rpx;
  margin-right:40rpx;
}
.detailValueTime{
  margin-left:40rpx;
  color:#333;
  font-size:32rpx;
}
.detailValueCopy{
  float:right;
  width:100rpx;
  height:50rpx;
  line-height: 50rpx;
  border:1px solid #fd9595;
  border-radius:6rpx;
  text-align: center;
  color:#fd9595;
  font-size:24rpx;
}
.evaluate{
  padding:60rpx;
  background-color: #fff;
  position:relative;
}
.evaluateTitle{
  font-size:28rpx;
  color:#b1b1b1;
  margin-bottom:60rpx;
}
.evaluateStars:after{
  content:"";
  display: block;
  clear:both;
}
.star{
  width:44rpx;
  height:44rpx;
  float:left;
  /* margin-right:40rpx; */
  padding: 0 20rpx;
}
.starLatest{
  margin-right:0;
}
.star image{
  width:100%;
}
.evaluateItem{
  margin-top:60rpx;
}
.itemCell{
  box-sizing: border-box;
  display: inline-block;
  /* width:140rpx; */
  padding:0 20rpx;
  height:50rpx;
  line-height: 50rpx;
  border:1px solid #666;
  border-radius:8rpx;
  text-align:center;
  font-size:24rpx;
  color:#666;
  margin-right:40rpx;
  margin-bottom:40rpx;
}
.bgColor{
  background-color: #fd9595;
  color:#fff;
  border:none;
}
.submit{
  background-color: #fff;
  padding-bottom: 60rpx;
}
.submitButton{
  background-color: #fd9595;
  border-radius: 8rpx;
  color:#fff;
  font-size:32rpx;
  width:440rpx;
  height:70rpx;
  line-height: 70rpx;
  margin:0 auto;
  text-align:center;
  /* opacity: 0.6; */
}
.checkbox{
  color:#333;
  font-size:32rpx;
  position:absolute;
  top:60rpx;
  right:60rpx;
   vertical-align: middle;
}
.checkbox .target{
  margin-right:10rpx;
  vertical-align: middle;
}
.textColor{
  color:#fd9595!important;
}



/*取消预约*/  
.cancel_toast_mask {  
  /* background-color: transparent;  */
  background: rgba(0, 0, 0, 0.5); 
  width: 100%;  
  height: 100%;  
  overflow: hidden;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 888;  
}  
/*toast*/  
.cancel_toast_content_box {  
  display: flex;  
  width: 100%;  
  height: 100%;  
  justify-content: center;  
  align-items: center;  
  position: fixed;  
  z-index: 999;  
}  
.cancel_toast_content {  
  width: 55%;  
  padding: 20rpx;    
  
}  
.cancel_toast_content-text{
  background-color: #fff; 
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  border-bottom:1px solid #d2d3d5;
}
.cancel_toast_content_text1 {  
  /* height: 100%;   */
  width: 100%;  
  padding-top:50rpx;
  padding-bottom:15rpx;
  color: #333;  
  font-size: 36rpx;  
  text-align: center; 
  font-weight: bold;
}
.cancel_toast_content_text2 {  
  /* height: 100%;   */
  width: 100%;  
  padding-top:15rpx;
  padding-bottom:30rpx;
  color: #999;  
  font-size: 30rpx;  
  text-align: center;  
  /* border-bottom:1px solid #d2d3d5; */
}
.cancel_toast_content_button{
  color:#333;
  font-size:28rpx;
  /* margin-top: 20rpx; */
  
  text-align: center;
  /* border-bottom:1px solid #d2d3d5; */
}
.cancel_toast_content_button1{
  box-sizing: border-box;
  display:inline-block;
  width:50%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  border-right:1px solid #d2d3d5; 
  border-bottom-left-radius:20rpx;
  color:#333;
  font-size:36rpx;
  background-color: #fff;
}
.cancel_toast_content_button2{
  box-sizing: border-box;
  display:inline-block;
  width:50%;
  height:100rpx;
  line-height:100rpx;
  text-align:center;
  border-bottom-right-radius:20rpx;
  color:#fd9595;
  font-size:36rpx;
  background-color: #fff;
  /* border:1px solid #d2d3d5; */
}