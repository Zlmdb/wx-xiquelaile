<view class='allContain'>
  <view class='item'>
    <view class='inputContain'>
      <label class='lableName'>姓名:</label><input placeholder-class="phcolor" class='nameEntry' type='text' placeholder='请输入姓名' bindinput="acceptValidate" value='{{maValue}}' style='width:260rpx;'></input>
    </view>
  </view>
  <view class='item'>
    <view class='inputContain' style=''>
      <label class='lableName'>手机号:</label><input placeholder-class="phcolor" class='nameEntry' type='number' placeholder='请输入手机号'  confirm-type="send" bindinput="acceptValue" value='{{phoneValue}}'></input>
    </view>
  </view>
  <view>
    <button class='submit' disabled='{{isMa}}' bindtap='modifySuccess'>确定</button>
  </view>
</view>
<!--mask-->  
<include src="../../utils/commonConfirm.wxml"/>
