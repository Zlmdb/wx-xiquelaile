@import "../../utils/commonConfirm.wxss";
.allContain{
  width:100%;
  height:100vh;
  background-color: #f5f5f5;
}
.item{
   height:120rpx;
   background-color: #fff;
   margin-bottom: 20rpx;
   position: relative;
}
.inputContain {
  /* width:580rpx; */
  position:absolute;
  top:50%;
  transform:translateY(-50%);
  /* margin:0 auto; */
  /* margin-bottom:70rpx; */
  padding-left: 70rpx;
  /* background-color: #fff; */
}
.lableName {
  display:inline-block;
  color:#333;
  font-size:36rpx;
  width:156rpx;
  vertical-align:middle;
}
.nameEntry {
  box-sizing:border-box;
  /* width:440rpx; */
  padding-left:20rpx;
  /* border-bottom:1px solid #eee; */
  display:inline-block;
  vertical-align:middle;
}
.phcolor{
  color:#b1b1b1;
  font-size:28rpx;
}
.send{
  width:200rpx;
  height:2rem;
  line-height: 2rem;
  color:#fff;
  font-size:28rpx;
  display: inline-block;
  vertical-align:middle;
  border:none;
  background-color: #fd9595;
}
.submit{
  width:690rpx;
  height:94rpx;
  line-height: 94rpx;
  color:#fff;
  background-color: #fd9595;
  border-radius: 10rpx;
  /* opacity:0.6; */
  font-size:36rpx;
  margin-top:50rpx;
}
button[disabled]{
  color:#fff!important;
  background-color: #fd9595!important;
  opacity:0.6; 
}


